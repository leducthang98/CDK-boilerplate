{
  "name": "cdk-boilerplate",
  "version": "0.1.0",
  "engines": {
    "node": "16.14.0",
    "yarn": "3.2.0"
  },
  "bin": "bin/cdk-boilerplate.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "makeApi": "npx ts-node script/generate-api-folder.ts",
    "gqlgen": "rm -f ./graphql/index.graphql && graphql-schema-utilities -s './graphql/*.graphql' -o './graphql/index.graphql' -d && graphql-codegen",
    "deploy": "npx cdk deploy --all --require-approval never",
    "destroy": "npx cdk destroy --all --force"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync-alpha": "2.20.0-alpha.0",
    "@aws-sdk/client-cloudwatch-logs": "3.78.0",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-resolvers": "2.6.4",
    "@middy/core": "2.5.7",
    "@prisma/client": "3.13.0",
    "amazon-cognito-identity-js": "5.2.8",
    "aws-cdk": "2.15.0",
    "aws-cdk-lib": "2.15.0",
    "aws-sdk": "2.1102.0",
    "axios": "0.26.1",
    "constructs": "10.0.114",
    "fs-extra": "10.1.0",
    "moment": "2.29.2",
    "node-fetch": "3.2.4",
    "nodemailer": "6.7.3",
    "path": "0.12.7",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.93",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "26.0.24",
    "@types/node": "10.17.27",
    "@types/nodemailer": "6.4.4",
    "@types/uuid": "8.3.4",
    "dotenv": "16.0.0",
    "esbuild": "0.11.23",
    "eslint-plugin-boundaries": "latest",
    "glob": "^8.0.3",
    "graphql-schema-utilities": "1.1.3",
    "jest": "26.6.3",
    "prettier": "2.6.1",
    "prisma": "3.13.0",
    "scripts": "*",
    "ts-node": "9.1.1",
    "tsconfig": "*",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.5.4"
  },
  "packageManager": "yarn@3.2.2"
}
